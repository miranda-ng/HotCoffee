<body class="body">

<!--MessageIn-->
<div class="box_in">
	<div class="head"><span class="date">%date% %time%</span><img class="IMG" src="images/message.gif"/>  %name%:</div>
	<div class="text" id="new_message">%text%</div>
	<script language="javascript">
		var juick = /(juick|jubo)/i;
                if (juick.test("%name%")){
               		juickonize("new_message");
			document.getElementById("new_message").id = "old_message";
		}
        </script>
</div>

