;------------------------------------------------------------------------

;main
@0=s$window:Main,ID=Background

;Groups
@1=s$hotclosedgroup:CL,ID=Row,Type=Group,Open=False,Hot=True
@2=s$normalclosedgroup:CL,ID=Row,Type=Group,Open=False
@3=s$hotopengroup:CL,ID=Row,Type=Group,Open=True,Hot=True
