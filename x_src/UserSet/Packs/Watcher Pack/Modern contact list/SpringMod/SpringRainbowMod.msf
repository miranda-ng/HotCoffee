;Buttons

;Common Select
@15=s$select:CL,ID=Selection

;Scroll Bar
@16=s$scrollBtnUpO:Main,ID=ScrollBar,Part=UpLineButton
@17=s$scrollBtnDnO:Main,ID=ScrollBar,Part=DownLineButton
@18=s$scrollThumb:Main,ID=ScrollBar,Part=Thumb
