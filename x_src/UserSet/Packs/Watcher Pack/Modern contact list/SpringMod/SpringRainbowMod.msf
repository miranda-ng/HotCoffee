[ModernSkin]

;Objects
  
;Main back
$window=sGlyph,Image,back.png,StretchBoth,10,210,10,40,255
$hotcontact=sGlyph,Image,contact.png,StretchBoth,3,3,3,3,150
;Groups
$hotopengroup=sGlyph,Image,group.png,StretchBoth,100,0,0,0,150
$normalopengroup=sGlyph,Image,group.png,StretchBoth,100,0,0,0,250
